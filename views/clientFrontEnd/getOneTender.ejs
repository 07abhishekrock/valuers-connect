<%- include("partials/header") %>

<div class="container tendind">
    <div class="tenderindividualseperate">
        <div class="row clienttendertotalheading tenderindividualboldheadng">
            <div class="col-lg-4">
                <p>TENDER DETAILS</p>
            </div>
            
        </div>
        <div>
            <div class="row clienttendercontentbodya">
                <div class="col-lg-4">
                    <a   href="" class="tenderindividualbold">  
                    TENDER ID                         
                    </a>
                </div>
                <div class="col-lg-8 ">                
                    <span><%=tender.tenderId%></span><br>
                </div>
            </div>
            <div class="row clienttendercontentbodya">
                <div class="col-lg-4">
                    <a   href="" class="tenderindividualbold">                           
                      EMD
                    </a>
                </div>
                <div class="col-lg-8 ">
                    <% if (tender.emd) {%>
                        <span><%=tender.emd%></span>
                      <%} else {%>
                        <span>N/A</span>
                      <%}%>                
                </div>
            </div>
            <div class="row clienttendercontentbodya">
                <div class="col-lg-4">
                    <a   href="" class="tenderindividualbold ">                           
                        Tendering Authority
                    </a>
                    
                </div>
                <div class="col-lg-8 "> 
                <%if(user.isMember){%>
                     <p><b><%=tender.tenderAuthority%></b> </p>

                    <%}else{%>
                     <a   href="#" style="color: blue;">Subscription Required To View Details</a><br>

                    <%}%>               
                </div>
            </div>
            <div class="row clienttendercontentbodya">
                <div class="col-lg-4">
                    <a   href="" class="tenderindividualbold">                           
                        Organization Detail
                    </a>
                </div>
                <div class="col-lg-8 ">                
                    <span> <%=tender.organizationDetail%></span><br>
                </div>
            </div>
            <div class="row ">
                <div class="col-lg-4">
                    <a   href="" class="tenderindividualbold lasttabletenderrow">                           
                        Brief
                    </a>
                </div>
                <div class="col-lg-8 " >                
            <span>&nbsp;<%=tender.brief%></span><br>
                </div>
            </div>
        </div>
    </div>
    
        

    <div class="tenderindividualseperate">
        <div class="row clienttendertotalheading tenderindividualboldheadng">
            <div class="col-lg-4">
                <p>Tender Documents</p>
            </div>
            
        </div>
        <div>
            <div class="row clienttendercontentbodya ">
                <div class="col-lg-4">
                    <a   href="" class="tenderindividualbold ">                           
                        Tender Document
                    </a>
                </div>
                <div class="col-lg-8 ">  
                    <%if(user.isMember){%>
                        <%if(tender.documents !== null && tender.documents !== "" && tender.documents !== undefined && tender.documents !== "undefined"){%>
                    <a   target="_blank"
                    rel="noopener noreferrer" href="<%=tender.documents%>" data-target="#exampleModalCenter" style="color: blue;">VIEW DOCUMENTS</a><br>
                    <%}else{%>
                        <div class="col-lg-8">
                            <p><b>N / A</b></p>
                          </div>
                          <%}%>
                    <%}else{%>
                     <a   href="#" style="color: blue;">Subscription Required To View Details</a><br>

                    <%}%>  
                </div>
            </div>
        </div> 
    </div>



    <div class="tenderindividualseperate">
        <div class="row clienttendertotalheading tenderindividualboldheadng">
            <div class="col-lg-4">
                <p>Key Dates *</p>
            </div>
            
        </div>
        <div>
            <div class="row clienttendercontentbodya">
                <div class="col-lg-4">
                    <a   href="" class="tenderindividualbold">                           
                        Last Date for Submission
                    </a>
                </div>
                <div class="col-lg-8 ">                
                    <span><%=tender.lastDate%></span><br>
                </div>
            </div>
            
            <div class="row ">
                <div class="col-lg-4">
                    <a   href="" class="tenderindividualbold lasttabletenderrow">                           
                        Opening Date
                    </a>
                </div>
                <div class="col-lg-8 ">                
                    <span>&nbsp;<%=tender.openingDate%></span><br>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="exampleModalCenter" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLongTitle">Modal title</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        ...
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary">Save changes</button>
      </div>
    </div>
  </div>
</div>


