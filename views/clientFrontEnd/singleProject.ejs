<%- include("partials/header") %>

<div class="container tendind" style="margin-top: 100px">
  <div class="tenderindividualseperate">
    <div>
      <div class="row clienttendercontentbodya">
        <div class="col-lg-4">
          <a href="" class="tenderindividualbold"> Listing ID </a>
        </div>
        <div class="col-lg-8"><span> <%=project.id%></span><br /></div>
      </div>
      <div class="row clienttendercontentbodya">
        <div class="col-lg-4">
          <a href="" class="tenderindividualbold"> Institution </a>
        </div>
        <div class="col-lg-8">
          <span> <%=project.institutionName%></span><br />
        </div>
      </div>
      <div class="row clienttendercontentbodya">
        <div class="col-lg-4">
          <a href="" class="tenderindividualbold"> Asset Type </a>
        </div>
        <div class="col-lg-8">
          <p><b><%=project.category%></b></p>
        </div>
      </div>

      <div class="row">
        <div class="col-lg-4">
          <a href="" class="tenderindividualbold lasttabletenderrow">
            Asset Location
          </a>
        </div>
        <div class="col-lg-8">
          <span><%=project.assetLocation%></span><br />
        </div>
      </div>

      <div class="row">
        <div class="col-lg-4">
          <a href="" class="tenderindividualbold lasttabletenderrow"> City </a>
        </div>
        <div class="col-lg-8"><span><%=project.assetCity%></span><br /></div>
      </div>

      <div class="row">
        <div class="col-lg-4">
          <a href="" class="tenderindividualbold lasttabletenderrow">
            Reserve Price
          </a>
        </div>
        <div class="col-lg-8">
          <span>â‚¹ <%=project.reservePrice%></span><br />
        </div>
      </div>

      <div class="row">
        <div class="col-lg-4">
          <a href="" class="tenderindividualbold lasttabletenderrow">
            Auction Date Time
          </a>
        </div>
        <div class="col-lg-8"><span><%=project.auctionDate%></span><br /></div>
      </div>
<br />
      <div class="row">
        <div class="col-lg-4">
          <a href="" class="tenderindividualbold lasttabletenderrow">
            Documents Available
          </a>
        </div>
        <%if(user.isMember){%> 
            <%if(project.projectDocuments !== null && project.projectDocuments !== "" && project.projectDocuments !== undefined && project.projectDocuments !== "undefined"){%>

        <div class="col-lg-8">
          <a
            target="_blank"
            rel="noopener noreferrer"
            href="<%=project.projectDocuments%>"
            ><span>View Documents</span></a
          ><br />
        </div>
        <%}else{%>
        <div class="col-lg-8">
          <p><b>N / A</b></p>
        </div>
        <%}%>
        <%}else{%>
        <div class="col-lg-8">
          <a href=""><span>Subscription Required To View Documents</span></a
          ><br />
        </div>
        <%}%>
      </div>
    </div>
  </div>

  <div class="tenderindividualseperate">
    <div>
      <div class="row clienttendercontentbodya">
        <div class="col-lg-4">
          <a href="" class="tenderindividualbold"> Last Date for Submission </a>
        </div>
        <div class="col-lg-8">
          <span><%=project.applicationDeadline%></span><br />
        </div>
      </div>

      <!-- <div class="row ">
                <div class="col-lg-4">
